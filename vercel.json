{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": [
                "GET",
                "OPTIONS",
                "PATCH",
                "DELETE",
                "POST",
                "PUT"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://blog-frontend-git-main-sdas2k3.vercel.app/",
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE"
            }
        },
    ]
}